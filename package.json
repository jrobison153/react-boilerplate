{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Debt stacking utility",
  "main": "",
  "scripts": {
    "start": "node server/src/cli_server",
    "preCommit": "gulp lint && gulp test-unit --singleRun && gulp build",
    "preCommit-debug": "gulp lint && gulp test-unit --singleRun && gulp build --debug",
    "build": "gulp build",
    "watch-debug": "./node_modules/.bin/nodemon --watch app/scripts --watch app/styles -e js,jsx,css --exec npm run preCommit-debug",
    "watch": "./node_modules/.bin/nodemon --watch app/scripts --watch app/styles -e js,jsx,css --exec npm run preCommit",
    "watch-build": "./node_modules/.bin/nodemon --watch app/scripts --watch app/styles -e js,jsx,css --exec gulp build",
    "watch-build-dev": "./node_modules/.bin/nodemon --watch app/scripts --watch app/styles -e js,jsx,css --exec gulp build --debug"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrobison153/react-boilerplate.git"
  },
  "keywords": [
    "sandbox",
    "react",
    "performance"
  ],
  "author": "Justin Robison",
  "license": "",
  "bugs": {
    "url": "https://github.com/jrobison153/react-boilerplate/issues"
  },
  "homepage": "https://github.com/jrobison153/react-boilerplate",
  "dependencies": {
    "bootstrap": "^3.3.4",
    "browserify": "^11.0.1",
    "express": "^4.12.3",
    "flux": "^2.0.3",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "normalize.css": "^3.0.3",
    "portscanner": "^1.0.0",
    "q": "^1.4.0",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel": "^5.1.13",
    "babelify": "^6.0.2",
    "chai": "^3.0.0",
    "chalk": "^1.0.0",
    "del": "^1.2.0",
    "glob": "^5.0.5",
    "gulp": "^3.8.11",
    "gulp-if": "^1.2.5",
    "gulp-jshint": "^1.10.0",
    "gulp-minify-css": "^1.1.6",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "jshint": "^2.7.0",
    "jshint-jsx": "^0.4.1",
    "jshint-stylish": "^1.0.1",
    "karma": "^0.12.36",
    "karma-browserify": "^4.2.1",
    "karma-chrome-launcher": "^0.1.12",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.2.1",
    "map-stream": "0.0.5",
    "minimist": "^1.1.1",
    "mocha": "^2.2.5",
    "nodemon": "^1.3.7",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.0",
    "selenium-standalone": "^4.5.3",
    "selenium-webdriver": "^2.46.1",
    "sinon": "^1.15.3",
    "through2": "^0.6.5",
    "uglify-js": "^2.4.23"
  }
}
